/* Write a program to print the fibonacci series.

Author: Onkarr19 

*/


#include <stdio.h>

int main(){
    
    int a,i,b,n,c;
    printf("Enter the number of elements you want :");
    scanf("%d",&n);
    a = 0;
    b = 1;
    
    printf("%d %d ",a,b);
    
    for(i=1;i<=n-2;i++){
        c = a + b;
        printf("%d ",c);
        a = b;
        b = c;
    }
    return 0;
}
